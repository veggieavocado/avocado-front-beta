{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "globals": {
      "document": true,
      "console": true,
      "window": true
    },
    "rules": {
      "no-console": 0,
      "linebreak-style": 0,
      "prefer-destructuring": 0,
      "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"]
    },
    "env": {
      "mocha": true
    }
}
