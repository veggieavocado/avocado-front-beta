    "immutable": "^3.8.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "particles.js": "^2.0.0",
    "raven": "^2.6.3",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.1",
